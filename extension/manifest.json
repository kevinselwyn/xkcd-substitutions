{
	"name": "xkcd: Substitutions",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Substitutions that make reading the news more fun (http://xkcd.com/1288/)",
	"homepage_url": "http://github.com/kevinselwyn/xkcd-substitutions",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"js/xkcd.substitutions.min.js",
				"js/xkcd.substitutions.app.js"
			]
		}
	]
}